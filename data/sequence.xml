<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_household" model="ir.sequence">
            <field name="name">Mã hộ gia đình</field>
            <field name="code">cmsw.household</field>
            <field name="prefix">000</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_bill" model="ir.sequence">
            <field name="name">Số hóa đơn</field>
            <field name="code">cmsw.bill</field>
            <field name="prefix"></field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>